{
  "name": "stake-frontend",
  "env": {
    "NEXT_PUBLIC_APP_NAME": "Stake Frontend",
    "NEXT_PUBLIC_APP_DESCRIPTION": "去中心化质押应用"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}